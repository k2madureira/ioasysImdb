  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:server": "NODE_ENV=development nodemon app.js",
    "dev:queue": "NODE_ENV=development nodemon queue.js",
    "db:generate": "NODE_ENV=development npx sequelize migration:generate",
    "db:migrate": "NODE_ENV=development npx sequelize db:migrate",
    "db:undo": "NODE_ENV=development npx sequelize db:migrate:undo",
    "test": "jest",
    "pretest": "NODE_ENV=test npx sequelize db:create && NODE_ENV=test npx sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "posttest": "NODE_ENV=test sequelize db:drop"
  },
  "devDependencies": {
    "eslint": "7.18.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "supertest": "6.1.3"
  },
  "dependencies": {
    "add": "2.0.6",
    "bcryptjs": "2.4.3",
    "bull": "3.20.1",
    "date-fns": "2.16.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "faker": "5.2.0",
    "handlebars": "4.7.6",
    "http-status-codes": "2.1.4",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
